{
  "base_url": "https://your-instance.wms.ocs.oraclecloud.com/your-tenant",
  "auth_method": "basic",
  "username": "${WMS_USERNAME}",
  "password": "${WMS_PASSWORD}",
  "company_code": "*",
  "facility_code": "*",
  "start_date": "2025-01-01T00:00:00Z",

  "pagination_mode": "sequenced",
  "page_size": 1000,

  "entities": [
    "order_hdr",
    "order_dtl",
    "allocation",
    "inventory",
    "item",
    "location",
    "wave",
    "task"
  ],

  "field_selection": {
    "order_hdr": ["id", "order_nbr", "type_id", "status_id", "company_id", "facility_id", "create_ts", "mod_ts", "priority"],
    "order_dtl": ["id", "order_hdr_id", "item_id", "order_qty", "allocated_qty", "pick_qty", "create_ts", "mod_ts", "status_id"],
    "allocation": ["id", "order_dtl_id", "item_id", "from_inventory_id", "to_inventory_id", "alloc_qty", "status_id", "create_ts", "mod_ts"],
    "inventory": ["id", "item_id", "location_id", "facility_id", "on_hand_qty", "allocated_qty", "available_qty", "status_id", "create_ts", "mod_ts"],
    "item": ["id", "item_name", "item_desc", "status_id", "item_type_id", "create_ts", "mod_ts"],
    "location": ["id", "locn_brcd", "zone_id", "facility_id", "status_id", "create_ts", "mod_ts"]
  },

  "entity_filters": {
    "order_hdr": {
      "status_id__in": "10,20,30,40",
      "facility_id__gt": 0
    },
    "order_dtl": {
      "status_id__in": "10,20,30",
      "order_qty__gt": 0
    },
    "allocation": {
      "status_id__in": "10,20,30,40,50,60,70,80,90",
      "alloc_qty__gt": 0
    },
    "inventory": {
      "facility_id__gt": 0,
      "on_hand_qty__gte": 0
    }
  },

  "schema_discovery_method": "auto",
  "log_level": "INFO"
}
